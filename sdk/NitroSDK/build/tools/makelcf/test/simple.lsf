#  Linker Command Script Spec File

Static main
{
	Address		0x02000000
	Object		*.o (.text)
	Object		*.axxx
	Object		OBJECT(func,*) (.text)
	Objects		bin/ARM9-TEG/Release/*.o
	Objects		"d:/Program Files/bin/ARM9-TEG/Release/*.o"
	Object		A/B/C
	Object		* GROUP(ROOT)
	Object		. ..
	Library		libos.a
	Libraries	lib/libmi.a

	StackSize	8192 1024
}

Autoload overlay_0
{
	Address		0x03f00000
	Object		aaa.o bbb.o
	Library		autoload_libos.a
	Libraries	lib/autoload_libmi.a
}

Autoload overlay_1
{
	After		main
	Object		ccc.o \
			ddd.o
	Library		autoload_libos.a
	Libraries	lib/autoload_libmi.a
}

Overlay overlay_2
{
	Group		GroupA
	After		overlay_1
	Object		eee.o fff.o
	Library		overlay_libos.a
	Libraries	lib/overlay_libmi.a
}

Overlay overlay_3
{
	After		overlay_1
	Object		eee.o fff.o
